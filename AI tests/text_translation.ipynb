    "from googletrans import Translator\n",
    "\n",
    "translator = Translator()\n",
    "\n",
    "def translate_text(text, dest='en'):\n",
    "    translation = translator.translate(text, dest=dest)\n",
    "    return translation.text, translation.src, translation.dest"
